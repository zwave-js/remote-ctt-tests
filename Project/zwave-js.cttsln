      <Items Id="665398" SectionId="51523" Number="CCR.1.1.1" Title="General Control Requirements" Level="2" ItemType="checkbox" Select="true" Support="false" Control="false" HoldsPortalResult="false" UploadResult="false" />
      <Items Id="665399" SectionId="51523" Number="CCR.1.1.1.1" Description="If a Command Class is controlled: The node implements the ability to interview, read and/or set other nodes supporting the Command Class. Nodes controlling Command Classes MAY use a subset of the Commands within a Command Class (for example only Set commands)." Note="CL:0000.00.13.01.1" Level="3" ItemType="checkbox" Select="true" Support="false" Control="false" HoldsPortalResult="false" UploadResult="false" />
      <Items Id="665400" SectionId="51523" Number="CCR.1.1.1.2" Header="&quot;Full&quot; control" Description="When a Command Class is marked as fully controlled during certification, the controlling node is said to implement &quot;full&quot; control of the actual Command Class and it MUST observe the requirements for the Command Class defined in the &lt;a href=&quot;/wiki/index.php?title=References&quot; target=&quot;_blank&quot;&gt;Command Class Control Specification&lt;/a&gt; ." Note="CL:0000.00.11.03.1" Level="3" ItemType="checkbox" Select="true" Support="false" Control="false" HoldsPortalResult="false" UploadResult="false" />
      <Items Id="665401" SectionId="51523" Number="CCR.1.1.1.3" Description="The &lt;a href=&quot;/wiki/index.php?title=References&quot; target=&quot;_blank&quot;&gt;Z-Wave Plus v2 Device Type Specification&lt;/a&gt; mandates some nodes to control a given set of Command Classes. It means that the controlling node MUST respect the requirements described in this document for the actual Command Classes." Note="CL:0000.00.11.04.1" Level="3" ItemType="checkbox" Select="true" Support="false" Control="false" HoldsPortalResult="false" UploadResult="false" />
      <Items Id="665402" SectionId="51523" Number="CCR.1.1.1.4" Header="Command Classes support discovery requirements" Description="A controlling node MUST read the capabilities and secure capabilities of a node/End Point once before trying to control a given Command Class." Note="CL:0000.00.11.05.1" Level="3" ItemType="checkbox" Select="true" Support="false" Control="false" HoldsPortalResult="false" UploadResult="false" />
      <Items Id="665403" SectionId="51523" Number="CCR.1.1.1.5" Description="A controlling node MUST read the Command Class version number of a supporting node using the Version Command Class prior to controlling the Command Class. There can be two exceptions to this rule:&#xA;&lt;br&gt;&#xA;      &lt;ul&gt;&#xA;        &lt;li&gt;If the controlling node controls only version 1 of a given Command Class, it may skip requesting the supporting node version for the actual Command Class.&lt;/li&gt;&#xA;        &lt;li&gt;A controlling node may issue version 1 commands in the supporting node interview before knowing the supporting node Command Class’ version. (e.g., when interviewing the Version Command Class itself)&lt;/li&gt;&#xA;      &lt;/ul&gt;" Note="CL:0000.00.11.06.2" Level="3" ItemType="checkbox" Select="true" Support="false" Control="false" HoldsPortalResult="false" UploadResult="false" />
      <Items Id="665404" SectionId="51523" Number="CCR.1.1.1.6" Description="A node controlling a given command class MUST allow an end user to control the command class in all listening supporting nodes present in the network operating with a security class that is granted to the controlling node." Note="CL:0000.00.11.07.1" Level="3" ItemType="checkbox" Select="true" Support="false" Control="false" HoldsPortalResult="false" UploadResult="false" />
      <Items Id="665405" SectionId="51523" Number="CCR.1.1.1.7" Description="A node controlling a Command Class MUST allow an end user to control a sleeping supporting node if it is the Wake Up destination." Note="CL:0000.00.11.08.1" Level="3" ItemType="checkbox" Selfcert="-" SelfcertType="select" VerificationType="select" AdhocType="select" Select="true" Support="false" Control="false" HoldsPortalResult="false" UploadResult="false">
        <ZatsData Key="CCR_ALLOW_CONTROL_OF_SLEEPING_NODES">
          <ZatsValues />
          <FormItemNumbers>
            <FormItemNumber>CCR.1.1.1.7</FormItemNumber>
          </FormItemNumbers>
          <ItemIds>
            <ItemId>665405</ItemId>
          </ItemIds>
        </ZatsData>
      </Items>
      <Items Id="665406" SectionId="51523" Number="CCR.1.1.1.8" Description="This interview MUST be performed prior to issuing any control command to the supporting node." Note="Preferably, it should be done during the commissioning phase of the supporting node or shortly after the inclusion of the controlling node.&#xA;&lt;br&gt;&lt;br&gt;&#xA;CL:0000.00.21.02.1" Level="3" ItemType="checkbox" Select="true" Support="false" Control="false" HoldsPortalResult="false" UploadResult="false" />
      <Items Id="665407" SectionId="51523" Number="CCR.1.1.1.9" Description="A “fully” controlling node MUST NOT skip or abort the interview of a Command Class if another independent Command Class could not be correctly interviewed." Note="CL:0000.00.21.04.1" Level="3" ItemType="checkbox" Selfcert="-" SelfcertType="select" VerificationType="select" AdhocType="select" Select="true" Support="false" Control="false" HoldsPortalResult="false" UploadResult="false">
        <ZatsTests Name="CCR_InterviewCompletion_Rev02">
          <ItemIds>665407</ItemIds>
        </ZatsTests>
      </Items>
      <Items Id="665408" SectionId="51523" Number="CCR.1.1.1.10" Description="A “partially” controlling node MAY skip steps in the specified frame flow if the queried data would not be used by the controller, or the data is retrieved from a separate source (i.e. database of Manufacturer Specific information and capabilities)." Note="CL:0000.00.23.02.1" Level="3" ItemType="checkbox" Select="true" Support="false" Control="false" HoldsPortalResult="false" UploadResult="false" />
      <Items Id="665409" SectionId="51523" Number="CCR.1.1.1.11" Description="Partial Control MAY be used for Application Command Classes only. Management and Transport-Encapsulation Command Class MUST be fully controlled." Note="CL:0000.00.11.14.1" Level="3" ItemType="checkbox" Select="true" Support="false" Control="false" HoldsPortalResult="false" UploadResult="false" />
      <Items Id="665410" SectionId="51523" Number="CCR.1.1.1.12" Description="The following order for reading Command Class capabilities is RECOMMENDED after discovering the Security Class and encapsulation capabilities of a node:&#xA;&#xA;      &lt;ul&gt;&#xA;        &lt;li&gt;Version Command Class&lt;/li&gt;&#xA;        &lt;li&gt;Z-Wave Plus Info Command Class&lt;/li&gt;&#xA;        &lt;li&gt;Wake Up Command Class&lt;/li&gt;&#xA;        &lt;li&gt;Association OR Multi Channel Association Command Class&lt;/li&gt;&#xA;        &lt;li&gt;Association Group Information (AGI) Command Class&lt;/li&gt;&#xA;        &lt;li&gt;Actuator Command Classes&lt;/li&gt;&#xA;        &lt;li&gt;Data reporting Command Classes&lt;/li&gt;&#xA;        &lt;li&gt;Multi Channel Command Class (and repeat the above order for each End Point, if any)&lt;/li&gt;&#xA;      &lt;/ul&gt;&#xA;      " Note="CL:0000.00.22.01.1" Level="3" ItemType="checkbox" Select="true" Support="false" Control="false" HoldsPortalResult="false" UploadResult="false" />
      <Items Id="665411" SectionId="51523" Number="CCR.1.1.1.13" Description="Some interviews or controlling scenarios may trigger no response in return to get type commands. In this case, it is RECOMMENDED to use timeouts according to the &lt;a href=&quot;/wiki/index.php?title=References&quot; target=&quot;_blank&quot;&gt;Z-Wave Plus Role Type Specification&lt;/a&gt;." Note="CL:0000.00.22.02.1" Level="3" ItemType="checkbox" Select="true" Support="false" Control="false" HoldsPortalResult="false" UploadResult="false" />
      <Items Id="665412" SectionId="51523" Number="CCR.1.1.1.14" Header="Minimum end user functionalities" Description="The minimum end user functionalities section in the &lt;a href=&quot;/wiki/index.php?title=References&quot; target=&quot;_blank&quot;&gt;Command Class Control Specification&lt;/a&gt; specifies what minimum set of actions an end user can perform on a supporting node when using the controlling node." Level="3" ItemType="checkbox" Select="true" Support="false" Control="false" HoldsPortalResult="false" UploadResult="false" />
      <Items Id="665413" SectionId="51523" Number="CCR.1.1.1.14.1" Description="Each section describing a required functionality indicates the command(s) that MUST be transmitted on the Z-Wave radio for full control." Note="CL:0000.00.31.01.1" Level="4" ItemType="checkbox" Select="true" Support="false" Control="false" HoldsPortalResult="false" UploadResult="false" />
      <Items Id="665414" SectionId="51523" Number="CCR.1.1.1.14.2" Description="Fields not present or described in the mandatory command(s) MAY be used freely by the controlling node either automatically or based on user input." Note="CL:0000.00.33.01.1" Level="4" ItemType="checkbox" Select="true" Support="false" Control="false" HoldsPortalResult="false" UploadResult="false" />
      <Items Id="665415" SectionId="51523" Number="CCR.1.1.1.14.3" Description="Fields indicated as &quot;user defined&quot; MUST be filled based on end user input. In this case, the end user MUST be able to select any value. (e.g. if the field is 2 bytes long, values from 0 to 65535 MUST be available)" Note="CL:0000.00.31.02.1" Level="4" ItemType="checkbox" Select="true" Support="false" Control="false" HoldsPortalResult="false" UploadResult="false" />
      <Items Id="665416" SectionId="51523" Number="CCR.1.1.1.14.4" Description="Fields indicated as &quot;User defined among supported&quot; MUST be filled based on user input and the supported values by the supporting node. The controlling node MUST allow the end user to set values supported by a supporting node even if it does not know what a given value represents." Note="CL:0000.00.31.03.1" Level="4" ItemType="checkbox" Select="true" Support="false" Control="false" HoldsPortalResult="false" UploadResult="false" />
      <Items Id="665417" SectionId="51523" Number="CCR.1.1.1.14.5" Description="Fields indicated as &quot;User defined among x..y and z&quot; MUST be filled based on user input and MUST be able to select between the indicated values." Note="CL:0000.00.31.04.1" Level="4" ItemType="checkbox" Select="true" Support="false" Control="false" HoldsPortalResult="false" UploadResult="false" />
      <Items Id="665418" SectionId="51523" Number="CCR.1.1.1.14.6" Description="Fields indicated as “User defined or x” MUST be filled either based on user input or automatically by the controller using the indicated value." Note="CL:0000.00.31.06.1" Level="4" ItemType="checkbox" Select="true" Support="false" Control="false" HoldsPortalResult="false" UploadResult="false" />
      <Items Id="665419" SectionId="51523" Number="CCR.1.1.1.14.7" Description="The product documentation MUST point out how to perform each action associated to the minimum end user functionalities required by a Command Class." Note="CL:0000.00.31.05.1" Level="4" ItemType="checkbox" Selfcert="-" SelfcertType="select" VerificationType="select" AdhocType="select" Select="true" Support="false" Control="false" HoldsPortalResult="false" UploadResult="false">
        <ZatsTests Name="CCR_DocuRequirement_Rev01">
          <ItemIds>665419</ItemIds>
        </ZatsTests>
      </Items>
      <Items Id="665420" SectionId="51523" Number="CCR.1.1.1.15" Header="Command Class control version" Description="Like a supporting node implements a version of its supported Command Classes, a controlling node MUST also implement a given version of a Command Class it controls. A node controlling version 3 of a Command Class will be able to control the functionalities of version 1, 2 and 3 supporting nodes." Note="CL:0000.00.11.09.1" Level="3" ItemType="checkbox" Select="true" Support="false" Control="false" HoldsPortalResult="false" UploadResult="false" />
      <Items Id="665421" SectionId="51523" Number="CCR.1.1.1.16" Description="When issuing commands, a controlling node MUST NOT use the fields described in the mandatory commands sections if they belong to a newer version than the version it claims to control.&#xA;&lt;br&gt;&lt;br&gt;&#xA;If a controlling node A controls a node B supporting a lower version, node A SHOULD still use the format (command payload) corresponding to the version node A controls, regardless of the version supported by node B.&#xA;&lt;br&gt;&lt;br&gt;&#xA;A controlling node A sending commands to a node B supporting a lower version MUST NOT use commands introduced in versions that are newer than Node B’s version.&#xA;&lt;br&gt;&lt;br&gt;&#xA;A node controlling a Command Class is not mandated to interview, provide minimum end user functionality or show node properties belonging to a newer version of the actual Command Class." Note="CL:0000.00.11.0A.2, CL:0000.00.12.02.1, CL:0000.00.11.0F.1" Level="3" ItemType="checkbox" Selfcert="-" SelfcertType="select" VerificationType="select" AdhocType="select" Select="true" Support="false" Control="false" HoldsPortalResult="false" UploadResult="false">
        <ZatsTests Name="CCR_ImplementedCCVersions_Rev02">
          <ItemIds>665421</ItemIds>
        </ZatsTests>
      </Items>
      <Items Id="665422" SectionId="51523" Number="CCR.1.1.1.17" Description="When a version is indicated in the mandatory interview or mandatory commands for end user functionalities, it MUST represent the minimum common version number between the supporting and controlling node. For example, with a version 4 controlling node and version 2 supporting node, the interview, minimum end user functionalities and properties MUST follow version 1 and 2 indications." Note="CL:0000.00.11.0B.1" Level="3" ItemType="checkbox" Select="true" Support="false" Control="false" HoldsPortalResult="false" UploadResult="false" />
      <Items Id="665423" SectionId="51523" Number="CCR.1.1.1.18" Header="Command Classes not present in the Command Class Control Specification" Description="A node MAY advertise that it controls a Command Class not (yet) defined in the &lt;a href=&quot;/wiki/index.php?title=References&quot; target=&quot;_blank&quot;&gt;Command Class Control Specification&lt;/a&gt; during certification." Note="CL:0000.00.13.02.1" Level="3" ItemType="checkbox" Select="true" Support="false" Control="false" HoldsPortalResult="false" UploadResult="false">
        <ZatsTests Name="CCR_NoControlDefinition_Rev01">
          <ItemIds>665423</ItemIds>
