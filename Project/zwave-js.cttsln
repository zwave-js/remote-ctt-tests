      </Items>
      <Items Id="664868" SectionId="51083" Number="CDR.19.2" Description="The transport service MUST be used only if the payload does not fit in the Z-Wave MAC frame size." Note="CC:0000.00.00.11.022" Level="1" ItemType="checkbox" SelfcertType="select" VerificationType="select" AdhocType="select" Select="true" Support="false" Control="false" HoldsPortalResult="false" UploadResult="false">
